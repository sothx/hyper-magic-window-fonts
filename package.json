{
	"name": "hyper-magic-window-fonts",
	"private": true,
	"type": "module",
	"version": "1.0.0",
	"engines": {
		"node": ">=20"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
			"onFail": "error"
		},
		"packageManager": {
			"name": "pnpm",
			"onFail": "error"
		}
	},
	"scripts": {
		"clean": "rimraf dist",
		"lint": "eslint . --no-fix",
		"build": "tsx ./src/main.ts",
		"format": "prettier . --write"
	},
	"dependencies": {
		"fontmin": "^2.0.0",
		"glob": "^11.0.3",
		"p-limit": "^6.2.0",
		"tsx": "^4.20.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.29.0",
		"@types/node": "^24.0.4",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.5.1",
		"jiti": "^2.4.2",
		"prettier": "^3.6.1",
		"rimraf": "^6.0.1",
		"typescript-eslint": "^8.35.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"ttf2woff2"
		]
	}
}
